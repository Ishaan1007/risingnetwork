# Google Meet API Configuration
# Copy this file to .env.local and fill in your actual values

# Google Meet API - Get from Google Cloud Console
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Development (localhost)
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/google/callback

# Production (Your Custom Domain) - Replace with your actual domain
GOOGLE_REDIRECT_URI=https://yourdomain.com/api/auth/google/callback

# Development
NEXT_PUBLIC_GOOGLE_MEET_ENABLED=true

# OneSignal Configuration
# Get from OneSignal dashboard (App Settings > Keys & IDs)
NEXT_PUBLIC_ONESIGNAL_APP_ID=88549a15-0843-4438-981f-cf6ab7a46b3c
NEXT_PUBLIC_ONESIGNAL_SAFARI_WEB_ID=web.onesignal.auto.35f9fdf2-e602-4832-95b7-1c199bdb2bd7
